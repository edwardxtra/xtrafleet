# QA Environment Local Configuration
# Copy to .env.local when working with QA environment locally

# Environment
NEXT_PUBLIC_ENV=qa

# Firebase (QA Project)
NEXT_PUBLIC_FIREBASE_API_KEY=your_qa_api_key
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your-qa-project.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your-qa-project-id
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your-qa-project.appspot.com
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_qa_sender_id
NEXT_PUBLIC_FIREBASE_APP_ID=your_qa_app_id

# Stripe (TEST KEYS - same as local dev)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_...
STRIPE_SECRET_KEY=sk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...

# Stripe Price IDs (QA/Test)
NEXT_PUBLIC_STRIPE_PRICE_MONTHLY=price_...
NEXT_PUBLIC_STRIPE_PRICE_SIX_MONTH=price_...
NEXT_PUBLIC_STRIPE_PRICE_ANNUAL=price_...

# Resend (Email) - Can use same key or separate for QA
RESEND_API_KEY=your_resend_key

# App URL (QA)
NEXT_PUBLIC_APP_URL=https://your-qa-project.web.app
